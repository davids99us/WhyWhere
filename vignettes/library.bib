@Article{Zachos2001a,
  Title                    = {{Trends, rhythms, and aberrations in global climate 65 Ma to present.}},
  Author                   = {Zachos, J and Pagani, M and Sloan, L and Thomas, E and Billups, K},
  Journal                  = {Science (New York, N.Y.)},

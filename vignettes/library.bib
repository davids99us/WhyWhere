@Article{Davey:1991yg,
  Title                    = {{Incorporating Wildlife Habitat Into An Ai Environment - Concepts, Theory, And Practicalities}},
  Author                   = {Davey, S M and Stockwell, D R B},
  Journal                  = {Ai Applications},
  Year                     = {1991},
  Number                   = {2},
  Volume                   = {5},

  Abstract                 = {The identification of wildlife habitat is becoming increasingly necessary in the planning and management of Australian native forests. Artificial intelligence (AI) systems offer forest managers an avenue to model wildlife habitat for the purpose of forest planning and management. To incorporate models of wildlife habitat into an AI environment, an understanding of niche and habitat theory is essential. We discuss such theories in relation to AI development. Using our understanding of habitat of the arboreal marsupials, we illustrate how the theory is used in the development of expert systems and decision support systems. For the purpose of forest planning and management of wildlife, habitat has to be thought of at different scales of resolutions; that is, at continental, regional, local, and site-specific scales. There are different practical and technical hurdles associated with the AI development and habitat modeling at each scale. This is important as there is a need to move between scales and extrapolate with confidence from location-specific data to non-inventoried sites, both spatially and through time. The lessons learned from our experience are discussed in terms of incorporating wildlife habitat into the Australian national forest inventory.},
  Annote                   = {ISI:A1991FX00200006},
  Url                      = {<Go to ISI>://A1991FX00200006}
}

@Article{Davey:1995yg,
  Title                    = {{Managing Biological Diversity With Intelligent Systems}},
  Author                   = {Davey, S M and Stockwell, D R B and Peters, D G},
  Journal                  = {Ai Applications},
  Year                     = {1995},
  Number                   = {2},
  Volume                   = {9},

  Abstract                 = {Management of biological diversity is an integral component of ecologically sustainable development and the international Convention on Biological Diversity. To manage biological diversity, government agencies must be able to locate diverse sources of information quickly and interpret these data intelligently. Advanced information management systems (e.g., machine learning and ''intelligent'' systems) can help governments do this. We developed an information strategy for management of biological diversity, using Australian forest systems as a case study. We used an advanced technology framework (open system architecture) supporting development and upgrading of information management facilities, access to electronic data through computer networks and information management systems, and agreed-upon information standards. Such an information environment must be relatively friendly for end users of the information, such as land-use decision and policy makers and resource managers. Our seven-layered open work-flow model integrates a number of intelligent information systems to help scientists provide timely and comprehensive advice on biological diversity. The model supplies a conceptual basis for the evolution of biology diversity information systems.},

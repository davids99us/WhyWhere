@Article{Lean:2009kx,
  Title                    = {{How will Earth's surface temperature change in future decades?}},
  Author                   = {Lean, Judith L and Rind, David H},
  Journal                  = {Geophys. Res. Lett.},
  Year                     = {2009},
  Volume                   = {36},

  Abstract                 = {Reliable forecasts of climate change in the immediate future are difficult, especially on regional scales, where natural climate variations may amplify or mitigate anthropogenic warming in ways that numerical models capture poorly. By decomposing recent observed surface temperatures into components associated with ENSO, volcanic and solar activity, and anthropogenic influences, we anticipate global and regional changes in the next two decades. From 2009 to 2014, projected rises in anthropogenic influences and solar irradiance will increase global surface temperature 0.15 $\backslash$pm0.03$\backslash$,\^{}\{$\backslash$circ\}C, at a rate 50\{\%\} greater than predicted by IPCC. But as a result of declining solar activity in the subsequent five years, average temperature in 2019 is only 0.03 $\backslash$pm0.01$\backslash$,\^{}\{$\backslash$circ\}C warmer than in 2014. This lack of overall warming is analogous to the period from 2002 to 2008 when decreasing solar irradiance also countered much of the anthropogenic warming. We further illustrate how a major volcanic eruption and a super ENSO would modify our global and regional temperature projections.},
  Keywords                 = {climate forecast; 1616 Geochemistry: Composition o},
  Publisher                = {American Geophysical Union},
  Url                      = {http://dx.doi.org/10.1029/2009GL038932}
}

@Article{Lean:2008gf,
  Title                    = {{How natural and anthropogenic influences alter global and regional surface temperatures: 1889 to 2006}},
  Author                   = {Lean, Judith L and Rind, David H},
  Journal                  = {Geophys. Res. Lett.},
  Year                     = {2008},
  Volume                   = {35},

  Abstract                 = {To distinguish between simultaneous natural and anthropogenic impacts on surface temperature, regionally as well as globally, we perform a robust multivariate analysis using the best available estimates of each together with the observed surface temperature record from 1889 to 2006. The results enable us to compare, for the first time from observations, the geographical distributions of responses to individual influences consistent with their global impacts. We find a response to solar forcing quite different from that reported in several papers published recently in this journal, and zonally averaged responses to both natural and anthropogenic forcings that differ distinctly from those indicated by the Intergovernmental Panel on Climate Change, whose conclusions depended on model simulations. Anthropogenic warming estimated directly from the historical observations is more pronounced between 45$\backslash$,\^{}\{$\backslash$circ\}S and 50$\backslash$,\^{}\{$\backslash$circ\}N than at higher latitudes whereas the model-simulated trends have minimum values in the tropics and increase steadily from 30 to 70$\backslash$,\^{}\{$\backslash$circ\}N.},
  Keywords                 = { Astrophysics, and Astronomy: Solar irradiance; 0370 Atmospheric,surface temperature; natural forcing; anthropogeni},
  Publisher                = {American Geophysical Union},
  Url                      = {http://dx.doi.org/10.1029/2008GL034864}
}

@Article{Leuliette:2009fr,
  Title                    = {{Closing the sea level rise budget with altimetry, Argo, and GRACE}},
  Author                   = {Leuliette, Eric W and Miller, Laury},
  Journal                  = {Geophys. Res. Lett.},
  Year                     = {2009},
  Volume                   = {36},

  Abstract                 = {An analysis of the steric and ocean mass components of sea level shows that the sea level rise budget for the period January 2004 to December 2007 can be closed. Using corrected and verified Jason-1 and Envisat altimetry observations of total sea level, upper ocean steric sea level from the Argo array, and ocean mass variations inferred from GRACE gravity mission observations, we find that the sum of steric sea level and the ocean mass component has a trend of 1.5 $\backslash$pm1.0 mm/a over the period, in agreement with the total sea level rise observed by either Jason-1 (2.4 $\backslash$pm1.1 mm/a) or Envisat (2.7 $\backslash$pm1.5 mm/a) within a 95\{\%\} confidence interval.},
  Keywords                 = {sea level rise; global warming; climate change; 45},

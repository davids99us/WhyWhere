@Article{Rahmstorf2007,
  Title                    = {{Recent climate observations compared to projections.}},
  Author                   = {Rahmstorf, Stefan and Cazenave, Anny and Church, John A and Hansen, James E and Keeling, Ralph F and Parker, David E and Somerville, Richard C J},

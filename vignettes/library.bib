@Article{Santer:2003yg,
  Title                    = {{Contributions of anthropogenic and natural forcing to recent tropopause height changes}},
  Author                   = {Santer, B D and Wehner, M F and Wigley, T M and Sausen, R and Meehl, G A and Taylor, K E and Ammann, C and Arblaster, J and Washington, W M and Boyle, J S and Bruggemann, W},
  Journal                  = {Science},
  Year                     = {2003},
  Number                   = {5632},
  Volume                   = {301},

  Abstract                 = {Observations indicate that the height of the tropopause-the boundary between the stratosphere and troposphere-has increased by several hundred meters since 1979. Comparable increases are evident in climate model experiments. The latter show that human-induced changes in ozone and well-mixed greenhouse gases account for approximately 80\{$\backslash$\}\{\%\} of the simulated rise in tropopause height over 1979-1999. Their primary contributions are through cooling of the stratosphere (caused by ozone) and warming of the troposphere (caused by well-mixed greenhouse gases). A model-predicted fingerprint of tropopause height changes is statistically detectable in two different observational (\{$\backslash$tt\{\}"\{\}\}reanalysis\{$\backslash$tt\{\}"\{\}\}) data sets. This positive detection result allows us to attribute overall tropopause height changes to a combination of anthropogenic and natural external forcings, with the anthropogenic component predominating.},
  Annote                   = {12881562},
  Url                      = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve\&db=PubMed\&dopt=Citation\&list\_uids=12881562}
}

@Article{Santer:2000yg,
  Title                    = {{Interpreting differential temperature trends at the surface and in the lower troposphere}},
  Author                   = {Santer, B D and Wigley, T M and Gaffen, D J and Bengtsson, L and Doutriaux, C and Boyle, J S and Esch, M and Hnilo, J J and Jones, P D and Meehl, G A and Roeckner, E and Taylor, K E and Wehner, M F},
  Journal                  = {Science},
  Year                     = {2000},
  Number                   = {5456},
  Volume                   = {287},

  Abstract                 = {Estimated global-scale temperature trends at Earth's surface (as recorded by thermometers) and in the lower troposphere (as monitored by satellites) diverge by up to 0.14 degrees C per decade over the period 1979 to 1998. Accounting for differences in the spatial coverage of satellite and surface measurements reduces this differential, but still leaves a statistically significant residual of roughly 0.1 degrees C per decade. Natural internal climate variability alone, as simulated in three state-of-the-art coupled atmosphere-ocean models, cannot completely explain this residual trend difference. A model forced by a combination of anthropogenic factors and volcanic aerosols yields surface-troposphere temperature trend differences closest to those observed.},
  Annote                   = {0010678823},
  Url                      = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve\&db=PubMed\&dopt=Citation\&list\_uids=0010678823}
}

@Article{Santer2003,
  Title                    = {{Influence of satellite data uncertainties on the detection of externally forced climate change}},
  Author                   = {Santer, B D and Wigley, T M and Meehl, G A and Wehner, M F and Mears, C and Schabel, M and Wentz, F J and Ammann, C and Arblaster, J and Bettge, T and Washington, W M and Taylor, K E and Boyle, J S and Bruggemann, W and Doutriaux, C},
  Journal                  = {Science},

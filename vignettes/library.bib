@Article{Palle:2005zr,
  Title                    = {{A multi-data comparison of shortwave climate forcing changes}},
  Author                   = {Pall\'{e}, E and Monta\~{n}\'{e}s-Rodriguez, P and Goode, P R and Koonin, S E and Wild, M and Casadio, S},
  Journal                  = {Geophys. Res. Lett.},

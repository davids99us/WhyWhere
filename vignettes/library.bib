@Article{Li2004,
  Title                    = {{Detecting and adjusting temporal inhomogeneity in Chinese mean surface air temperature data}},
  Author                   = {Li, Qingxiang and Liu, Xiaoning and Zhang, Hongzheng and {Thomas C.}, Peterson and {David R.}, Easterling},
  Journal                  = {Advances in Atmospheric Sciences},
